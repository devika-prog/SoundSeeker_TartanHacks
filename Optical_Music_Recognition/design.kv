windowManager:
	MainPage:
	InputPage:
	OutputPage:
	
<MainPage>:
	name: "Main"
	RelativeLayout:
		Image:
			id: image
			source: "./MainPage.png"
			pos_hint : {'center_x':.5, 'center_y':.5}
			size_hint: (1.5,1)
		Button:
			font_size:'30sp'
			on_release: 
				app.root.current="Input"
				root.manager.transition.direction = 'left'
			font_name: "Acme"
			size_hint: (.4,.15)
			pos_hint : {'center_x':.5, 'center_y':.305}
			background_color: (105/255, 125/255, 250/250, 0)
			
<InputPage>:
	name: "Input"
	id: my_widget
	
	RelativeLayout:		
		Label:
			font_size:'30sp'
			text: "Select A File to Continue"
			pos_hint : {'center_x':.5, 'center_y':.9}
			font_name: "Acme"
		Button:
			text: "Run"
			background_normal: ''
			font_size:'20sp'
			on_release: 
				app.root.current="Output"
				root.manager.transition.direction = 'left'
			pos_hint : {'center_x':.85, 'center_y':.9}
			size_hint: (.2,.11)
			background_color: (242/255, 107/255, 167/250, 1)
			color: (0,0, 0, 1)
			font_name: "Acme"
		Button:
			text: "Main"
			background_normal: ''
			font_size:'20sp'
			on_release: 
				app.root.current="Main"
				root.manager.transition.direction = 'right'
			pos_hint : {'center_x':.15, 'center_y':.9}
			size_hint: (.2,.11)
			background_color: (242/255, 107/255, 167/250, 1)
			color: (0,0, 0, 1)
			font_name: "Acme"
		Image:
			id: image
			source: ""
			pos_hint : {'center_x':.5, 'center_y':.5}
			size_hint: (1,.5)
			color: [34/255, 42/255, 91/250, 1]
		FileChooserIconView:
			id: fileChooser
			on_selection: my_widget.selected(fileChooser.selection)
			pos_hint : {'center_x':.5, 'center_y':.5}
			size_hint: (1,.5)
			background_color: (105/255, 125/255, 250/250, 1)
		Image:
			id: image
			source: ""
			pos_hint : {'center_x':.5, 'center_y':.2}
			size_hint: (1,.4)
			color: [255/255, 255/255, 255/255, 1]
		Image:
			id: image
			source: ""
			pos_hint : {'center_x':.5, 'center_y':.2}
			size_hint: (1,.4)
			

<OutputPage>:
	name: "Output"
	
	RelativeLayout:
		
		Label:
			text: "Labeled Notes"
			font_size:'30sp'
			pos_hint : {'center_x':.5, 'center_y':.9}
			font_name: "Acme"
		Button:
			text: "Back"
			background_normal: ''
			font_size:'20sp'
			on_release: 
				app.root.current="Input"
				root.manager.transition.direction = 'right'
			pos_hint : {'center_x':.15, 'center_y':.93}
			size_hint: (.2,.11)
			background_color: (242/255, 107/255, 167/250, 1)
			color: (0,0, 0, 1)
			font_name: "Acme"
		Image:
			id: image1
			source: ""
			pos_hint : {'center_x':.5, 'center_y':.7}
			size_hint: (1,.3)
			color: [255/255, 255/255, 255/255, 1]
		Image:
			id: image
			source: "./detected.png"
			pos_hint : {'center_x':.5, 'center_y':.7}
			size_hint: (1,.3)
		Label:
			text: "To Hear Your Music, Play the Notes Below:"
			font_size:'20sp'
			pos_hint : {'center_x':.5, 'center_y':.47}
			font_name: "Acme"
		Image:
